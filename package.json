{
  "name": "@mj-studio/http-client",
  "version": "1.0.2",
  "description": "Custom React hook and Rest adapter for network call",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/mym0404/http-client",
  "scripts": {
    "prepare": "yarn c",
    "c": "yarn lint && yarn tsc && yarn jest --coverage -u",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "test": "jest",
    "p": "yarn publish --access=public"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/mym0404/http-client"
  },
  "author": "MJ Studio <mym0404@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@mj-studio/js-util": "1.0.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "@dooboo/eslint-config": "1.3.0",
    "@types/eslint": "8.4.1",
    "@types/jest": "26.0.24",
    "eslint": "8.8.0",
    "import-sort-style-eslint": "6.0.0",
    "jest": "26.6.3",
    "jest-fetch-mock": "3.0.3",
    "prettier": "2.5.1",
    "prettier-plugin-import-sort": "0.0.7",
    "typescript": "4.4.4",
    "ts-jest": "26.5.6"
  },
  "importSort": {
    ".js, .jsx": {
      "style": "eslint",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint",
      "options": {}
    }
  },
  "files": [
    "dist"
  ]
